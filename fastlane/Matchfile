git_url("https://github.com/CoilSiteInc/certificates.git") # sshのurlではなく、httpsで指定してます。
git_basic_authorization(Base64.strict_encode64("#{ENV['MATCH_REPOSITORY_ACCOUNT']}:#{ENV['MATCH_PERSONAL_ACCESS_TOKEN']}"))
storage_mode("git")
type("appstore") # The default type, can be: appstore, adhoc, enterprise or development

app_identifier(["com.DefaultCompan.CISample"]) # bundleIDを指定しよう
username("taisuke99@i.softbank.jp") # Your Apple Developer Portal username